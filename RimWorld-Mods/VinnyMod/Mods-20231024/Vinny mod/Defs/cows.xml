      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
      <headPosPerRotation>
        <li>(0.02, 0, 0.23)</li>
        <li>(0.35, 0, 0.05)</li>
        <li>(0.0, 0, -0.185)</li>
        <li>(-0.35, 0, 0.05)</li>
      </headPosPerRotation>
    </race>
    <tradeTags>
      <li>AnimalCommon</li>
      <li>AnimalPet</li>
    </tradeTags>
    <tools>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>0.1</power>
        <cooldownTime>0.05</cooldownTime>
        <linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>1</amount>
            </li>
            <li>
              <def>EMP</def>
              <amount>1</amount>
            </li>
            <li>
              <def>Flame</def>
              <amount>1</amount>
              <chance>0.7</chance>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>0.1</power>
        <cooldownTime>0.05</cooldownTime>
        <linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>1</amount>
            </li>
            <li>
              <def>EMP</def>
              <amount>1</amount>
            </li>
            <li>
              <def>Flame</def>
              <amount>1</amount>
              <chance>0.7</chance>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
    </tools>
  </ThingDef>
  
